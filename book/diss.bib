@article{Boden.1993,
 abstract = {Im vorliegenden Beitrag wird die Sprachkontaktsituation zwischen dem Mittelniederdeutschen und dem Festland-Skandinavischen zur Zeit der Hanse untersucht, wobei die theoretischen Ans{\"a}tze der genetischen Linguistik und die beiden Transfertypen beim Sprachkontakt verwendet werden. Traditionelle Annahmen hinsichtlich dieser Sprachkontaktsituation, zum Beispiel im Hinblick auf Mischsprache, Pidginisierung und {\dq}Halbsprachigkeit{\dq} (semicommunication), werden untersucht und auf der Grundlage ihres soziolinguistischen und/oder strukturellen Mangels an Plausibilit{\"a}t zur{\"u}ckgewiesen. Es wird ein soziolinguistisch einleuchtendes Sprachkontaktmodell vorgeschlagen und auf andere Sprachkontaktsituationen angewendet, an denen germanische Sprachen mit besserer {\"U}berlieferungslage beteiligt sind, das sind Altnorwegisch-Altenglisch und Altenglisch/Mittelenglisch-Altfranz{\"o}sisch. Die Parallelen zwischen diesen beiden Kontaktsituationen und derjenigen des Mittelniederdeutschen mit dem Skandinavischen legen die Vermutung nahe, da{\ss} die bisherigen Annahmen {\"u}ber die Auswirkungen des Kontakts zwischen dem Mittelniederdeutschen und den skandinavischen Sprachen {\"u}bertrieben waren, und da{\ss} die morphosyntaktischen Vereinfachungen innerhalb des Festland-Skandinavischen eher durch sprachinterne Vorg{\"a}nge ausgel{\"o}st als durch mittelniederdeutschen Einflu{\ss} beschleunigt wurden.},
 author = {Boden, Keith},
 year = {1993},
 title = {{A Re-Examination of Middle Low German-Scandinavian Language Contact}},
 url = {http://www.jstor.org/stable/40503856},
 pages = {p 292-306},
 volume = {60},
 number = {3},
 issn = {00441449},
 journal = {{Zeitschrift f{\"u}r Dialektologie und Linguistik}}
}


